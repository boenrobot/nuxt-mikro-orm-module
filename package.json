{
  "name": "nuxt-mikro-orm-module",
  "version": "0.4.0",
  "description": "Nuxt MikroORM module",
  "license": "LGPL-2.1-only",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "workspaces": [
    "./test/fixtures/*"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "cross-env-shell CONSOLA_LEVEL=4 NODE_OPTIONS=\"--enable-source-maps\" nuxi dev playground --inspect",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "doc": "yarn run typedoc",
    "format": "prettier --write ./src ./test",
    "lint": "eslint --fix .",
    "prepare": "husky || echo \"\"",
    "chores": "yarn run format && yarn run lint && yarn run format && yarn run doc",
    "release": "yarn run chores && yarn run test && yarn run prepack && changelogen --release && npm publish && git push --follow-tags",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.11.2",
    "defu": "^6.1.4"
  },
  "peerDependencies": {
    "@mikro-orm/core": "*",
    "h3": "^1.11.1",
    "nitropack": "^2.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@mikro-orm/core": "*",
    "@mikro-orm/mysql": "*",
    "@nuxt/devtools": "^1.3.3",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.7.1",
    "@nuxt/schema": "^3.12.1",
    "@nuxt/test-utils": "^3.13.1",
    "@types/node": "^20.14.2",
    "changelogen": "^0.5.5",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.9",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "nuxi": "^3.11.1",
    "nuxt": "^3.11.2",
    "prettier": "^3.3.2",
    "typedoc": "^0.25.13",
    "typescript": "latest",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2",
    "vue-tsc": "^2.0.19"
  },
  "packageManager": "yarn@4.3.0+sha256.d7f2fd514a5862009753c52798cc48ae602d181c09341cf2ff8b9c935dc3db1a"
}
